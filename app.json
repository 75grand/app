{
  "expo": {
    "name": "75grand",
    "slug": "75grand",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "grand",
    "updates": {
      "useClassicUpdates": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "zone.jero.grand"
    },
    "android": {
      "adaptiveIcon": {
        "monochromeImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0369A1"
      },
      "package": "zone.jero.grand"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#0369A1"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "81bd5b0d-5aa1-457e-8059-362776c19a1b"
      }
    },
    "owner": "75grand"
  }
}
